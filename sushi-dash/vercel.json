{
  "buildCommand": "prisma generate --schema=server/prisma/schema.prisma && vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install && cd server && npm install",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
